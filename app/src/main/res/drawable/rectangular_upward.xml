<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape>

        <stroke
            android:width="1dp"
            android:color="@color/cleargreyColor" />
        <corners android:topLeftRadius="3dp"
            android:topRightRadius="3dp"/>
    </shape>
</item>
<item
    android:bottom="0dp"
    android:left="2dp"
    android:right="2dp"
    android:top="2dp">
    <shape>
        <solid android:color="@color/white" />
        <padding
            android:bottom="5dp"
            android:left="5dp"
            android:right="5dp"
            android:top="5dp" />
    </shape>
</item>
</layer-list>
